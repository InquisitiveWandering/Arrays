#include <stdio.h>

int main() {
    //size of array
        int n;
        printf("Enter the size of the array: ");
        scanf("%d",&n);
    //scanning the elements
        int a[n];
        printf("Enter all the elements of the array:\n");
        for(int i=0; i<n; i++)
        {
            scanf("%d",&a[i]);
        }
    //printing the elements
        printf("The elements of the array are:\n");
        for(int i=0; i<n; i++)
        {
            printf("%d\n",a[i]);
        }
    return 0;
}
